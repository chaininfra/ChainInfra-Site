{
  "redirects": [
    {
      "source": "/tools",
      "destination": "/live_data",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/api/validator-metrics",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "Surrogate-Control",
          "value": "no-store"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "no-cache"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ],
  "functions": {
    "src/app/api/validator-metrics/route.ts": {
      "maxDuration": 30
    }
  }
}